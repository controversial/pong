{
  "extends": "airbnb",
  "env": {
    "browser": "true"
  },
  "rules": {
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "global-require": "off",
    "no-param-reassign": "off",
    "no-console": "off",
  },
  "settings": {
    "import/resolver": "webpack"
  }
}
